<h2>Models</h2>

<table>
  <tr>
    <th>Model</th>
    <th>Retail</th>
  </tr>
  <% @models.each do |model| %>
    <tr>
      <td><%= link_to(model.name, model) %></td>
      <td><%= Nretail.where(model_id: model).sum :number %></td>
    </tr>
  <% end %>
</table>

<% if user_signed_in? %>
  <p><%= link_to "Create new", new_model_path %></p>
<% end %>


<p><%= link_to "Home", root_path %></p>